{
    "name": "jorgesanabria/dbal",
    "description": "Simple DBAL for PHP",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "DBAL\\": "DBAL/"
        },
        "files": [
            "DBAL/Hooks/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DBAL\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "psr/log": "^1",
        "webonyx/graphql-php": "^15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
